@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/iccl/ontologies/3Coloring> .

<http://www.semanticweb.org/iccl/ontologies/3Coloring> rdf:type :Ontology ;
                                                       
                                                       rdfs:comment "Some simple axioms defining the 3Colorability problem of a given graph structure." .


#################################################################
#
#    Object Properties
#
#################################################################


###  http://www.semanticweb.org/iccl/ontologies/3Coloring#edge

:edge rdf:type :IrreflexiveProperty ,
               :ObjectProperty ,
               :SymmetricProperty .





#################################################################
#
#    Classes
#
#################################################################


###  http://www.semanticweb.org/iccl/ontologies/3Coloring#BlueNode

:BlueNode rdf:type :Class ;
          
          rdfs:subClassOf :Node ,
                          [ rdf:type :Restriction ;
                            :onProperty :edge ;
                            :allValuesFrom [ rdf:type :Class ;
                                             :unionOf ( :GreenNode
                                                        :RedNode
                                                      )
                                           ]
                          ] .



###  http://www.semanticweb.org/iccl/ontologies/3Coloring#GreenNode

:GreenNode rdf:type :Class ;
           
           rdfs:subClassOf :Node ,
                           [ rdf:type :Restriction ;
                             :onProperty :edge ;
                             :allValuesFrom [ rdf:type :Class ;
                                              :unionOf ( :BlueNode
                                                         :RedNode
                                                       )
                                            ]
                           ] .



###  http://www.semanticweb.org/iccl/ontologies/3Coloring#Node

:Node rdf:type :Class ;
      
      rdfs:subClassOf [ rdf:type :Class ;
                        :unionOf ( :BlueNode
                                   :GreenNode
                                   :RedNode
                                 )
                      ] .



###  http://www.semanticweb.org/iccl/ontologies/3Coloring#RedNode

:RedNode rdf:type :Class ;
         
         rdfs:subClassOf :Node ,
                         [ rdf:type :Restriction ;
                           :onProperty :edge ;
                           :allValuesFrom [ rdf:type :Class ;
                                            :unionOf ( :BlueNode
                                                       :GreenNode
                                                     )
                                          ]
                         ] .





#################################################################
#
#    Individuals
#
#################################################################


###  http://www.semanticweb.org/iccl/ontologies/3Coloring#n1

:n1 rdf:type :Node ,
             :NamedIndividual ;
    
    :edge :n2 ,
          :n3 .



###  http://www.semanticweb.org/iccl/ontologies/3Coloring#n2

:n2 rdf:type :Node ,
             :NamedIndividual ;
    
    :edge :n3 ,
          :n4 .



###  http://www.semanticweb.org/iccl/ontologies/3Coloring#n3

:n3 rdf:type :Node ,
             :NamedIndividual .



###  http://www.semanticweb.org/iccl/ontologies/3Coloring#n4

:n4 rdf:type :Node ,
             :NamedIndividual .





#################################################################
#
#    General axioms
#
#################################################################


[ rdf:type :AllDisjointClasses ;
  :members ( :BlueNode
             :GreenNode
             :RedNode
           )
] .




